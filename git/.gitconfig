[user]
  name  = hachibu
  email = raymondsohn@gmail.com

[core]
  editor       = vim
  excludesfile = ~/.gitignore

[color]
  ui     = auto
  status = auto
  diff   = auto
  branch = auto

[pager]
  branch = false

[pull]
  rebase = true

[push]
  default = current

[alias]
  fix = commit -a --amend --cleanup=strip -C HEAD
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
