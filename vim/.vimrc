" set nocompatible
" filetype off
" filetype plugin indent on
" syntax on

" set backspace=indent,eol,start
" set clipboard=unnamed
" set colorcolumn=81
" set expandtab
" set history=10000
" set hlsearch
" set incsearch
" set nolist
" set listchars=trail:~
" set mouse=a
" set nofoldenable
" set noswapfile
" set nowrap
" set number
" set shiftwidth=2
" set smartindent
" set softtabstop=2
" set t_Co=256
" set tabstop=2
" set title
" set updatetime=500
" set virtualedit=all
" set wildmode=list:longest,list:full

" highlight LineNr ctermfg=grey

" nnoremap <C-J> <C-W><C-J>
" nnoremap <C-K> <C-W><C-K>
" nnoremap <C-L> <C-W><C-L>
" nnoremap <C-H> <C-W><C-H>
